{
  "name": "@ew-iam/role-definition-client",
  "version": "1.0.0",
  "description": "",
  "main": "dist/role-definition-client.js",
  "module": "dist/role-definition-client.esm.js",
  "types": "dist/src/role-definition-client.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:typechain && pnpm run build:ts && copyfiles \"ethers/**/*.d.ts\" dist",
    "build:ts": "bili",
    "build:typechain": "pnpm run build:typechain:resolver && pnpm run build:typechain:registry",
    "build:typechain:resolver": "typechain --target ethers-v4 --outDir ethers './node_modules/@ew-iam/contracts/artifacts/contracts/RoleDefinitionResolver.sol/RoleDefinitionResolver.json'",
    "build:typechain:registry": "typechain --target ethers-v4 --outDir ethers './node_modules/@ew-iam/contracts/artifacts/@ensdomains/ens/contracts/ENSRegistry.sol/ENSRegistry.json'",
    "test:watch": "npm run start-rpc -- \"jest --coverage --watchAll\"",
    "test": "npm run start-rpc -- \"jest --coverage --no-cache\""
  },
  "keywords": [],
  "author": "Energy Web",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@ew-iam/contracts": "~1.0.0",
    "ethers": "4.0.45",
    "tslib": "~2.1.0"
  },
  "devDependencies": {
    "@typechain/ethers-v4": "^4.0.0",
    "bili": "~5.0.5",
    "copyfiles": "~2.4.1",
    "rollup-plugin-typescript2": "~0.30.0",
    "typechain": "^4.0.3",
    "typescript": "^4.2.3",
    "jest": "~26.6.3",
    "@types/jest": "~26.0.21"
  }
}